
development:
  adapter: postgresql
  encoding: unicode
  database: prospector_development
  pool: 5
  username: dev
  password: dev
  host: localhost

# Warning: The database defined as "test" will be erased and
# re-generated from your development database when you run "rake".
# Do not set this db to the same as development or production.
test:
  adapter: postgresql
  encoding: unicode
  database: prospector_test
  pool: 5
  username: dev
  password: dev
  host: localhost

# Note that in production on most Rails-compatible PaaS services,
